{
    "family": "api-mock-%SUBDOMAIN_NAME%",
    "containerDefinitions": [
        {
            "image": "715994263731.dkr.ecr.eu-central-1.amazonaws.com/parku/docs:%BRANCH_NAME%",
            "name": "api-mock-%SUBDOMAIN_NAME%",
            "environment": [
                {
                    "name": "SERVICE_NAME",
                    "value": "mockserver"
                },
                {
                    "name": "SERVICE_TAGS",
                    "value": "{\"branch\": \"%SUBDOMAIN_NAME%\"}"
                }
            ],
            "cpu": 10,
            "memory": 256,
            "essential": true,
            "portMappings": [
                {
                    "containerPort": 80
                }
            ]
        }
    ]
}
